{
	"name": "icanboogie/http",
	"type": "library",
	"description": "Provides an API to handle HTTP requests.",
	"keywords": [
		"http",
		"dispatcher",
		"request",
		"response"
	],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/HTTP/issues",
		"source": "https://github.com/ICanBoogie/HTTP"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.5",
		"icanboogie/prototype": "^2.3",
		"icanboogie/event": "^1.4|^2.0",
		"icanboogie/datetime": "^1.2"
	},
	"require-dev": {
		"icanboogie/event": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\HTTP\\": "lib/"
		},
		"classmap": [
			"lib/RescueEvent.php"
		],
		"files": [
			"helpers.php"
		]
	}
}
